{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitReturns": false,
    "outDir": "./dist"
  },
  "include": ["src/**/*", "environment.d.ts"],
  "exclude": ["node_modules"]
}
